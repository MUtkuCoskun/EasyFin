    {"id":"D.MARGIN.GROSS",      "formula":"=3D / 3C",                 "desc":"Brüt Kar Marjı (çeyrek)"},
    {"id":"D.MARGIN.EBITDA",     "formula":"=(3D+3DA+3DB+3DC+4B)/3C",  "desc":"FAVÖK Marjı (çeyrek)"},
    {"id":"D.MARGIN.NET",        "formula":"=3Z / 3C",                  "desc":"Net Kar Marjı (çeyrek)"},
    {"id":"D.RATIO.CURRENT",     "formula":"=1A / 2A",                  "desc":"Cari Oran (nokta)"},
    {"id":"D.DEBT.TOTAL",        "formula":"=2AA + 2BA",                "desc":"Toplam Finansal Borç (nokta)"},
    {"id":"D.DEBT.NET",          "formula":"=(2AA+2BA) - 1AA",          "desc":"Net Borç (nokta)"},
    {"id":"D.TTM.SALES",         "formula":"=TTM(3C)",                  "desc":"TTM Satışlar"},
    {"id":"D.TTM.EBITDA",        "formula":"=TTM(EBITDA)",              "desc":"TTM FAVÖK"},
    {"id":"D.TTM.NETINCOME",     "formula":"=TTM(3Z)",                  "desc":"TTM Ana Ortaklık Net Karı"},

    {"id":"D.VAL.PE",            "formula":"=MCAP / TTM(3Z)",           "desc":"F/K"},
    {"id":"D.VAL.PS",            "formula":"=MCAP / TTM(3C)",           "desc":"Fiyat/Satış"},
    {"id":"D.VAL.PB",            "formula":"=MCAP / 2O[last]",          "desc":"PD/DD"},
    {"id":"D.VAL.EVEBITDA",      "formula":"=(MCAP + NET_DEBT[last]) / TTM(EBITDA)","desc":"FD/FAVÖK"},
    {"id":"D.VAL.NETDEBT_EBITDA","formula":"=NET_DEBT[last] / TTM(EBITDA)","desc":"Net Borç/FAVÖK"}
